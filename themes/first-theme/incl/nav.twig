<nav class="navbar">
           {% if pages["_meta"].meta.logo %}
        <div class="logo">
            <a href="{{ base_url }}">
                <img src="{{ base_url }}/{{ pages["_meta"].meta.logo|url }}" alt="" />
            </a>
        </div>
        {% endif %}
        
    <ul class="nav-main_list no-bullets">
        {% for page in pages(depthOffset=-1) if page.title and not page.hidden %}
            <li class="nav-link">
                <a class="{{ 'nav-link_btn' }}{% if page.id == current_page.id %} active_btn{% endif %}" href="{{ page.url }}">{{ page.title }}</a>
            </li>
        {% endfor %}
    </ul>
 
</nav>
